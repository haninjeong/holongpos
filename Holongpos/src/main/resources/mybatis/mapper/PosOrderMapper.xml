<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.springboot.jdbc.IOrderMenuDAO">
	<insert id="insertOrderMenu">
		INSERT into ordermenu
		 VALUES(SEQ_ORDERMENU.nextval
		 ,#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7},sysdate,
		seq_receipt.nextval)
	</insert>
	


</mapper>	